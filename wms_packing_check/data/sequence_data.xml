<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Packing Check Records -->
    <data noupdate="1">
        <record id="seq_wms_packing_check" model="ir.sequence">
            <field name="name">WMS Packing Check</field>
            <field name="code">wms.packing.check</field>
            <field name="prefix">PKC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>