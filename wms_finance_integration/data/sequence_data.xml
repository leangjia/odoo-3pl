<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Financial Transaction Sequence -->
    <record id="seq_wms_financial_transaction" model="ir.sequence">
        <field name="name">Financial Transaction</field>
        <field name="code">wms.financial.transaction</field>
        <field name="prefix">FTR</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Cost Allocation Sequence -->
    <record id="seq_wms_cost_allocation" model="ir.sequence">
        <field name="name">Cost Allocation</field>
        <field name="code">wms.cost.allocation</field>
        <field name="prefix">CAL</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Financial Report Sequence -->
    <record id="seq_wms_financial_report" model="ir.sequence">
        <field name="name">Financial Report</field>
        <field name="code">wms.financial.report</field>
        <field name="prefix">FRE</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>